<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>SearchServlet</servlet-name>
		<servlet-class>dev.java.SearchServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>EditServlet</servlet-name>
		<servlet-class>dev.java.EditServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SearchServlet</servlet-name>
		<url-pattern>/expenses_db/Search</url-pattern>
	</servlet-mapping>	
	<servlet-mapping>
		<servlet-name>EditServlet</servlet-name>
		<url-pattern>/expenses_db/Edit</url-pattern>
	</servlet-mapping>

</web-app>